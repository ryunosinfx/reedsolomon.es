{
	"root": true,
	"env": {
		"es6": true,
		"browser": true,
		"node": false
	},
	"parser": "babel-eslint",
	"rules": {
		// "prettier/prettier": [1, { "trailingComma": "es6", "singleQuote": true, "semi": true }],
		"curly": 0,
		// "comma-dangle": [2, "never"],
		"comma-spacing": 0,
		"eqeqeq": [2, "allow-null"],
		"key-spacing": 0,
		"no-underscore-dangle": 0,
		"no-unused-expressions": 0,
		"no-shadow": 0,
		"no-shadow-restricted-names": 0,
		"no-extend-native": 0,
		"no-var": 2,
		"new-cap": 0,
		"semi-spacing": 0,
		"space-unary-ops": 0,
		"space-infix-ops": 0,
		"consistent-return": 0,
		"strict": 0,
		"quotes": [2, "single"],
		"semi": [2, "always"],
		"linebreak-style": [2, "unix"]
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2017
	},
	"plugins": [],
    "globals": {
        "o": "writable",
        "var2": "readonly"
    }
}
